extends layouts/main

block content
    .form-container
        form(action=`/videos${routes.upload}`, method="post")
            label(for="video") 동영상
            input(type="file",id="video",name="file",required=true)
            input(type="text",name="title",placeholder="제목",required=true)
            textarea(name="description",placeholder="설명",required=true)
            input(type="submit",values="Upload Video")
            

        