extends layouts/main

block content
    .form-container
        form(action=`/videos${routes,editVideo}`, method="post")
            input(type="text",name="title",placeholder="제목")
            textarea(type="text",name="description",placeholder="설명")
            input(type="submit",values="Upload Video")
    a.form-container__link--delete.form-container__link(href=`/videos${routes.deleteVideo}`) Delete Video
            

        