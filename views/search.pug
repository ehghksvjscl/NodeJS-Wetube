extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 검색 : #{searchingBy}
    .search__videos
        if video.length === 0
            h4 검색된 비디오가 없습니다.
        each item in video
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl
            })